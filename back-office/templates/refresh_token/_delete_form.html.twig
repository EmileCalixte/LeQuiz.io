<form method="post" action="{{ path('refresh_token_delete', {'token': refresh_token.token}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ refresh_token.token) }}">
    <button class="btn">Delete</button>
</form>
