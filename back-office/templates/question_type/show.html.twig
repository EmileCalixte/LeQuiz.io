{% extends 'base.html.twig' %}

{% block title %}QuestionType{% endblock %}

{% block body %}
    <h1>QuestionType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ question_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ question_type.name }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ question_type.label }}</td>
            </tr>
            <tr>
                <th>Ischild</th>
                <td>{{ question_type.ischild ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ question_type.createdAt ? question_type.createdAt|date('Y-m-d H:i:s T') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ question_type.updatedAt ? question_type.updatedAt|date('Y-m-d H:i:s T') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('question_type_index') }}">back to list</a>

    <a href="{{ path('question_type_edit', {'id': question_type.id}) }}">edit</a>

    {{ include('question_type/_delete_form.html.twig') }}
{% endblock %}
